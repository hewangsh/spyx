<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hws.product.mapper.ProductMapper">

    <select id="getProductId" resultType="com.hws.model.entity.product.Product">
        select *
        from product
        where
        id = #{productId}
    </select>
</mapper>